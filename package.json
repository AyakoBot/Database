    "@swc/core": "1.15.8",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^25.0.7",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
