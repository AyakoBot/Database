  secret    String? @db.VarChar

  @@id([guildId])
  @@index([appId])
}

model level {
  userid     String    @db.VarChar
  guildid    String    @default("1") @db.VarChar
  type       LevelType
  xp         Decimal   @default(0) @db.Decimal
  multiplier Decimal   @default(1) @db.Decimal

  @@id([userid, guildid, type])
  @@index([guildid])
}

model levelchannels {
  userid    String  @db.VarChar
  guildid   String  @db.VarChar
  channelid String  @db.VarChar
  xp        Decimal @default(0) @db.Decimal

  @@id([userid, guildid, channelid])
  @@index([guildid])
}

model leveling {
  xppermin Decimal @default(1) @db.Decimal
  xppermsg Decimal @default(1) @db.Decimal

  guildid          String      @id @db.VarChar
  active           Boolean     @default(true)
  xpmultiplier     Decimal     @default(1) @db.Decimal
  rolemode         Boolean     @default(false)
  lvlupdeltimeout  Decimal     @default(10) @db.Decimal
  lvlupchannels    String[]    @db.VarChar
  lvlupemotes      String[]    @db.VarChar
  embed            Decimal?    @db.Decimal
  ignoreprefixes   Boolean     @default(false)
  prefixes         String[]
  minwords         Decimal     @default(0) @db.Decimal
  lvlupmode        LevelUpMode @default(silent)
  blchannelid      String[]    @db.VarChar
  blroleid         String[]    @db.VarChar
  bluserid         String[]    @db.VarChar
  wlchannelid      String[]    @db.VarChar
  wlroleid         String[]    @db.VarChar
  wluserid         String[]    @db.VarChar
  excludeBots      Boolean     @default(true)
  minParticipants  Decimal     @default(2) @db.Decimal
  requireUnmute    Boolean     @default(true)
  lvluptext        String?
  textenabled      Boolean     @default(true)
  voiceenabled     Boolean     @default(true)
  pingUser         Boolean     @default(false)
  cooldown         Decimal     @default(60) @db.Decimal
  cooldownType     Boolean     @default(false)
  msgXpRangeBottom Decimal     @default(15) @db.Decimal
  msgXpRangeTop    Decimal     @default(25) @db.Decimal
  vcXpRangeBottom  Decimal     @default(0) @db.Decimal
  vcXpRangeTop     Decimal     @default(10) @db.Decimal
  formulaType      FormulaType @default(polynomial)
  curveModifier    Decimal     @default(100) @db.Decimal

  @@index([guildid])
}

enum FormulaType {
  logarithmic
  linear
  quadratic
  cubic
  polynomial
  exponential
}

model levelingmultichannels {
  guildid         String   @db.VarChar
