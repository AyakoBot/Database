model votesettings {
  uniquetimestamp     Decimal  @id @db.Decimal
  guildid             String   @db.VarChar
  token               String?
  reminders           Boolean  @default(false)
  announcementchannel String?  @db.VarChar
  active              Boolean  @default(false)
  linkedid            Decimal? @db.Decimal
  name                String?

  @@index([guildid, uniquetimestamp])
}

model welcome {
  guildid      String   @db.VarChar
  channelid    String?  @db.VarChar
  active       Boolean  @default(false)
  embed        Decimal? @db.Decimal
  pingroles    String[] @db.VarChar
  pingusers    String[] @db.VarChar
  pingjoin     Boolean  @default(false)
  gifChannelId String?  @db.VarChar

  @@id(guildid)
  @@index([guildid])
}

model WelcomeGIF {
  guildId   String @db.VarChar
  channelId String @db.VarChar
  msgId     String @db.VarChar
  url       String @db.Text

  @@id([msgId, url])
  @@index(guildId)
  @@index(msgId)
}

model deletethreads {
  guildid    String  @db.VarChar
  channelid  String  @db.VarChar
  deletetime Decimal @db.Decimal

  @@id(channelid)
  @@index([guildid])
}

model shop {
  guildid       String  @id @db.VarChar
  active        Boolean @default(false)
  currencyemote String?

  @@index([guildid])
}

model shopitems {
  guildid         String   @db.VarChar
  roles           String[] @db.VarChar
  price           Decimal? @db.Decimal
  uniquetimestamp Decimal  @id @db.Decimal
  shoptype        ShopType @default(command)
  buttontext      String?
  buttonemote     String?
  msgid           String?  @db.VarChar
  channelid       String?  @db.VarChar
  active          Boolean  @default(false)

  @@index([guildid, uniquetimestamp])
}

model shopusers {
  userid    String   @db.VarChar
  guildid   String   @db.VarChar
  boughtids String[] @db.VarChar

  @@id([userid, guildid])
  @@index([userid, guildid])
}

model voicehubs {
  guildid         String   @db.VarChar
  channelid       String?  @db.VarChar
  active          Boolean  @default(false)
  blroleid        String[] @db.VarChar
  bluserid        String[] @db.VarChar
  categoryid      String?  @db.VarChar
  uniquetimestamp Decimal  @id @db.Decimal
  wlroleid        String[] @db.VarChar
  wluserid        String[] @db.VarChar
  deletetime      Decimal  @default(3600) @db.Decimal
  private         Boolean  @default(true)

  @@index([guildid, uniquetimestamp])
}

model voicechannels {
  ownerid          String   @db.VarChar
  guildid          String   @db.VarChar
  channelid        String   @db.VarChar
  everyonelefttime Decimal? @db.Decimal
  hubid            String   @db.VarChar

  @@id([guildid, channelid])
  @@index([guildid, channelid])
}

model antiraid {
  guildid        String                 @id @db.VarChar
  active         Boolean                @default(false)
  posttof        Boolean                @default(false)
  pingroles      String[]               @db.VarChar
  pingusers      String[]               @db.VarChar
  timeout        Decimal                @default(15) @db.Decimal
  jointhreshold  Decimal                @default(5) @db.Decimal
  actiontof      Boolean                @default(true)
  action         AntiRaidPunishmentType @default(kick)
  postchannels   String[]               @db.VarChar
  disableinvites Boolean                @default(true)

  @@index([guildid])
}

model features {
  title       String @id @db.VarChar
  subtitle    String @db.VarChar
  image       String @db.VarChar
  placeholder String @db.VarChar

  @@index([title])
}

model heartbeats {
  ms        Decimal @db.Decimal
  shard     Decimal @id @db.Decimal
  timestamp Decimal @db.Decimal

  @@index([shard])
}

model users {
  userid         String   @id @db.VarChar
  username       String   @default("Unkown User")
  avatar         String   @default("https://cdn.discordapp.com/embed/avatars/0.png")
  socials        String[]
  socialstype    String[]
  lastfetch      Decimal  @default(1) @db.Decimal
  ptremindersent Boolean  @default(false)
  votereminders  Boolean  @default(true)
  apiToken       String?
  shrtUrlsAccess Boolean  @default(false)
  displayName    String   @default("Unkown User")
  tokens         tokens[]

  @@index([userid])
}

model tokens {
  userid       String   @db.VarChar
  botid        String   @db.VarChar
  accesstoken  String?  @db.VarChar
  refreshtoken String?  @db.VarChar
  expires      Decimal? @db.Decimal
  scopes       String[]
  user         users    @relation(fields: [userid], references: [userid])

  @@id([userid, botid])
}

model linkedRolesDeco {
  roleId          String?  @db.VarChar
  uniquetimestamp Decimal  @id @db.Decimal
  guildid         String   @db.VarChar
  allowedUsers    String[] @db.VarChar
  allowedRoles    String[] @db.VarChar
  botId           String?  @db.VarChar
  botSecret       String?  @db.VarChar
  botToken        String?  @db.VarChar

  @@index([uniquetimestamp])
}

model linkedRoleTokens {
  botId  String @db.VarChar
  userId String @db.VarChar
  token  String @db.VarChar

  @@id([botId, userId])
}

model noCommandsGuilds {
  guildId String @id @db.VarChar

  @@index([guildId])
}

model blockingUsers {
  userId  String  @id @db.VarChar
  created Decimal @db.Decimal

  @@index([userId])
}

model urlScans {
  url     String
  id      String  @id
  done    Boolean @default(false)
  expires Decimal @db.Decimal

  @@index([url])
}

model shrtUrls {
  id      String        @id
  userId  String        @db.VarChar
  forward String
  uses    shrtUrlUses[]
}

model shrtUrlUses {
  id        String
  timestamp Decimal  @id @db.Decimal
  url       shrtUrls @relation(fields: [id], references: [id])
}

model voiceStateUpdateQueue {
  userId  String  @db.VarChar
  guildId String  @db.VarChar
  deaf    Boolean
  mute    Boolean

  @@id([userId, guildId])
}

model event {
  userId  String  @db.VarChar
  identId String  @db.VarChar
  hits    Int     @default(0)
  has     Boolean @default(false)

  @@id([identId, userId])
}

model pingReport {
  guildid         String   @db.VarChar
  active          Boolean  @default(false)
  channelIds      String[] @db.VarChar
  cooldown        Decimal  @default(10) @db.Decimal
  roleId          String?
  uniquetimestamp Decimal  @id @db.Decimal

  @@index([roleId])
}

model votePunish {
  guildid              String                   @db.VarChar
  active               Boolean                  @default(false)
  roleId               String?                  @db.VarChar
  reqRoleAmount        Decimal?                 @default(2) @db.Decimal
  uniquetimestamp      Decimal                  @id @db.Decimal
  channelIds           String[]                 @db.VarChar
  cooldown             Decimal                  @default(30) @db.Decimal
  deleteMessageSeconds Decimal                  @default(604800) @db.Decimal
  duration             Decimal                  @default(86400) @db.Decimal
  punishment           AutoPunishPunishmentType @default(tempmute)
  reqRoles             String[]                 @db.VarChar
  blroleid             String[]                 @db.VarChar
  bluserid             String[]                 @db.VarChar
  voteInitRoles        String[]                 @db.VarChar

  @@index([guildid])
}

model Reminder {
  userId    String  @db.VarChar
  channelId String  @db.VarChar
  reason    String
  startTime Decimal @id @db.Decimal
  endTime   Decimal @db.Decimal

  @@index([userId, startTime])
}

model ticketing {
  uniquetimestamp     Decimal    @id @db.Decimal
  guildid             String     @db.VarChar
  active              Boolean    @default(false)
  logChannelIds       String[]   @db.VarChar
  archiveCategoryId   String?    @db.VarChar
  type                TicketType @default(Thread)
  // Only need on dmToThread & Thread
  archiveDuration     Decimal    @default(60) @db.Decimal
  channelId           String?    @db.VarChar
  // Only need on dmToChannel & Channel
  categoryId          String?    @db.VarChar
  // Only need on dmToThread & dmToChannel
  sendMessagePrefixes String[]   @db.Text

  mentionRoles String[] @db.VarChar
  mentionUsers String[] @db.VarChar

  bluserid String[]   @db.VarChar
  blroleid String[]   @db.VarChar
  DMTicket DMTicket[]

  @@index(guildid)
  @@index(type)
}

model DMTicket {
  dmId       String  @id @db.VarChar
  userId     String  @unique @db.VarChar
  channelId  String  @unique @db.VarChar
  settingsId Decimal @db.Decimal

  settings ticketing @relation(fields: [settingsId], references: [uniquetimestamp])

  @@index(dmId)
  @@index(channelId)
}

enum TicketType {
  dmToThread
  dmToChannel
  Thread
  Channel
}

enum AutoPunishPunishmentType {
  warn
  kick
  tempmute
  ban
  tempban
  channelban
  tempchannelban
  softban
}

enum PunishmentType {
  warn
  kick
  tempmute
  ban
  tempban
  channelban
  tempchannelban
  strike
  softban
}

enum StoredPunishmentTypes {
  warn
  kick
  tempmute
  ban
  tempban
  channelban
  tempchannelban
  softban
  vctempmute
  vctempdeaf
  vcdeaf
  vcmute
  mute
}

enum AntiRaidPunishmentType {
  kick
  ban
}

enum AnswerType {
  paragraph
  short
  number
  boolean
  multiple_choice
  single_choice
  text
}

enum LevelType {
  guild
  global
}

enum LevelUpMode {
  messages
  reactions
  silent
}

enum VoteType {
  guild
  bot
}

enum ShopType {
  command
  message
}

enum WeekendType {
  onlyOnWeekend
  onlyOnWeekdays
  everyDay
}

enum ArtType {
  full
  icon
  emoji
}

enum AppealStatus {
  pending
  accepted
  rejected
}
