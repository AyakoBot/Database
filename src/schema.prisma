}

model expiry {
  guildid         String   @id(map: "modsettings_pkey") @db.VarChar
  bans            Boolean  @default(false)
  channelbans     Boolean  @default(false)
  kicks           Boolean  @default(false)
  mutes           Boolean  @default(false)
